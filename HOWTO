cd build/
cmake ../
make


### win32 target on archlinux

install with pacman:
community/mingw32-gcc
community/mingw32-runtime
community/mingw32-w32api
community/mingw32-binutils
community/mingw32-pthreads

install with AUR:
mingw32-zlib
mingw32-openssl
mingw32-qt

mkdir build-win32
cd build-win32
CC=/usr/bin/i486-mingw32-gcc CXX=/usr/bin/i486-mingw32-g++ cmake ..
