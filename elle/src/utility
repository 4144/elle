// -*- c++ -*-
#ifndef ELLE_UTILITY
# define ELLE_UTILITY

# include <ostream>
# include_next <utility>

namespace std
{
  template <class K, class V>
  ostream&
  operator <<(ostream& out,
              pair<K, V> const& p)
  {
    return out << "(" << p.first <<  ", " << p.second << ")";
  }
}

#endif
